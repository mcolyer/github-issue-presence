{
  "manifest_version": 2,

  "name": "Github Issue Presence",
  "description": "Provides presence to github issues",
  "version": "1.0",

  "content_scripts" : [
      {
        "matches" : [
          "https://github.com/*/*/issues*"
        ],
        "js" : ["contentscript.js"],
        "run_at" : "document_idle",
        "all_frames" : false
      }
    ]
}
